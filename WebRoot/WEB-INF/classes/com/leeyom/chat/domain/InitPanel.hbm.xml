<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.leeyom.chat.domain">

    <class name="InitPanel" table="initPanel">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="code" type="java.lang.Integer"/>
        <property name="msg" type="java.lang.String"/>
        <property name="panalDataId" type="java.lang.Integer"/>
    </class>

</hibernate-mapping>
